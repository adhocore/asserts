{
    "name": "adhocore/asserts",
    "description": "More PHPUnit assertions as trait",
    "type": "library",
    "keywords": ["php","asserts","phpunit","json-subset"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jitendra Adhikari",
            "email": "jiten.adhikary@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ahc\\Asserts\\": "src/"
        },
        "files": []
    },
    "autoload-dev": {
        "psr-4": {
            "Ahc\\Asserts\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        }
    },
    "scripts": {
        "post-root-package-install": [
        ],
        "test": "vendor/bin/phpunit --coverage-text --coverage-clover coverage.xml"
    }
}
